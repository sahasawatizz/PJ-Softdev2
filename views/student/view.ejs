<div class="profileBox">
    <img src="https://img.icons8.com/ios/150/000000/administrator-male--v1.png"/>
    <h1>Name : <%= one.name %> </h1>
    <h2>ID. : <%= one.id %> </h2>
</div>
<div class="stuTable">
  <table class="table table-striped">
      <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">info</th>
          <th scope="col">detail </th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <th scope="row">1</th>
          <td>Tel</td>
          <td><%= one.tel %> </td>
        </tr>
        <tr>
          <th scope="row">2</th>
          <td>Address</td>
          <td><%= one.address %> </td>
        </tr>
        <tr>
          <th scope="row">3</th>
          <td>Food</td>
          <td><%= one.food %> </td>
        </tr>
        <tr>
          <th scope="row">4</th>
          <td>Grade</td>
          <td><%= one.grade %> </td>
        </tr>
        <tr>
          <th scope="row">5</th>
          <td>Nation</td>
          <td><%= one.nation %> </td>
        </tr>
      </tbody>
    </table>
    <h1 style="text-align: center;">Status : <%= one.status %> </h1>
</div>