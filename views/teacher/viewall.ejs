<h1 style="text-align: center; margin-top: 30px;">All Students</h1>
<div class="tableDiv">
  <table class="table table-striped">
    <thead>
      <tr>
        <th scope="col">ID.</th>
        <th scope="col">Name</th>
        <th scope="col">tel</th>
        <th scope="col">address</th>
        <th scope="col">food</th>
        <th scope="col">grade</th>
        <th scope="col">nation</th>
        <th scope="col">Status</th>
      </tr>
    </thead>
    <tbody>
        <%  student.forEach(element => { %>
            <tr class="head">
                <th scope="row"><%= element.id %></th>
                <td><%= element.name %></td>
                <td><%= element.tel %></td>
                <td><%= element.address %></td>
                <td><%= element.food %></td>
                <td><%= element.grade %></td>
                <td><%= element.nation %></td>
                <td><%= element.status %></td> 
                <td><a href="/teacher/edit/<%= element._id %> "><img style="margin: 0px 5px;" src="https://img.icons8.com/material-outlined/30/000000/edit.png"/></a>
                  <a href="/teacher/delete/<%= element._id %> "><img style="margin: 0px 5px;" src="https://img.icons8.com/ios-glyphs/30/000000/filled-trash.png"/></a></td>
              </tr>
        <%}) %>
    </tbody>
  </table>
</div>
